{
  "name": "crypto-rewards-comparison-turkey",
  "version": "1.0.0",
  "description": "A website to compare staking, campaign, and other rewards offered by crypto exchanges and DeFi programs available in Turkey.",
  "scripts": {
    "dev": "next dev",
    "build": "npm install --force --production=false && next build",
    "start": "next start",
    "lint": "next lint",
    "backend": "python backend/app.py",
    "dev:backend": "cd backend && python app.py",
    "dev:frontend": "next dev",
    "dev:all": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "scraper:start": "./start-scraper.sh",
    "scraper:stop": "./stop-scraper.sh",
    "scraper:check": "./check-scraper.sh",
    "scraper:run": "cd backend && python scraper_service.py"
  },
  "keywords": [
    "crypto",
    "rewards",
    "staking",
    "turkey",
    "comparison"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@headlessui/react": "2.2.3",
    "@heroicons/react": "2.2.0",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "aos": "2.3.4",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.2",
    "chart.js": "4.4.9",
    "framer-motion": "12.11.3",
    "gsap": "3.13.0",
    "jspdf": "3.0.1",
    "jspdf-autotable": "5.0.2",
    "lottie-react": "2.4.1",
    "next": "^14.0.4",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-chartjs-2": "5.3.0",
    "react-countup": "6.5.3",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "9.16.0",
    "react-slick": "0.30.3",
    "react-sparklines": "^1.7.0",
    "react-spring": "9.7.5",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
